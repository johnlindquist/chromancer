{
  "name": "chromancer",
  "version": "1.0.0",
  "description": "Command-line interface for Chrome automation using DevTools Protocol",
  "main": "dist/index.js",
  "bin": {
    "chromancer": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepack": "npm run clean && npm run build",
    "test": "node test/unit-tests.js",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "chrome",
    "chromium",
    "devtools",
    "cdp",
    "automation",
    "scraping",
    "browser",
    "cli",
    "puppeteer",
    "testing",
    "web-automation"
  ],
  "author": "John Lindquist",
  "license": "MIT",
  "homepage": "https://github.com/johnlindquist/chromancer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnlindquist/chromancer.git"
  },
  "bugs": {
    "url": "https://github.com/johnlindquist/chromancer/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@oclif/core": "^4.3.1",
    "@oclif/plugin-help": "^6.2.28",
    "@types/node": "^22.15.29",
    "oclif": "^4.18.0",
    "puppeteer-core": "^24.10.0",
    "typescript": "^5.8.3"
  },
  "oclif": {
    "bin": "chromancer",
    "dirname": "chromancer",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ]
}